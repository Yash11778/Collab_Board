{
  "version": 2,
  "builds": [
    {
      "src": "hackathon/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "hackathon/dist"
      }
    },
    {
      "src": "backend/api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/hackathon/dist/$1"
    }
  ]
}
