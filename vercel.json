{
  "version": 2,
  "installCommand": "npm install --prefix hackathon && npm install --prefix .",
  "buildCommand": "npm run build --prefix hackathon",
  "outputDirectory": "hackathon/dist",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
