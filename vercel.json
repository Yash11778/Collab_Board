{
  "version": 2,
  "builds": [
    {
      "src": "hackathon/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "hackathon/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://your-backend-deployment.vercel.app/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/hackathon/$1"
    }
  ],
  "outputDirectory": "hackathon/dist"
}
