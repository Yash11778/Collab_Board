{
  "buildCommand": "cd hackathon && npm ci && npm run build",
  "outputDirectory": "hackathon/dist",
  "installCommand": "npm ci --include=dev",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
